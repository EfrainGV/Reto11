<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.udea.PrimaryController">
   <children>
      <ImageView fitHeight="324.0" fitWidth="346.0">
         <image>
            <Image url="@Logo.png" />
         </image>
      </ImageView>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Iniciar" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
